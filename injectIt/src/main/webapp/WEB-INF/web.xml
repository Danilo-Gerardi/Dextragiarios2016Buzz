<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Inject it</display-name>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>


  <servlet-mapping>
    <servlet-name>inject-servlet</servlet-name>
    <url-pattern>/api/usuarios</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>inject-servlet</servlet-name>
    <servlet-class>com.dextra.injectit.servlets.InjectServlet</servlet-class>
  </servlet>
</web-app>
